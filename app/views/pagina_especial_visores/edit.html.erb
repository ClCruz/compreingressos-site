<h1>Editing pagina_especial_visor</h1>

<% form_for(@pagina_especial_visor) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :blank %><br />
    <%= f.check_box :blank %>
  </p>
  <p>
    <%= f.label :ordem %><br />
    <%= f.text_field :ordem %>
  </p>
  <p>
    <%= f.label :imagem_file_name %><br />
    <%= f.text_field :imagem_file_name %>
  </p>
  <p>
    <%= f.label :imagem_content_type %><br />
    <%= f.text_field :imagem_content_type %>
  </p>
  <p>
    <%= f.label :imagem_file_size %><br />
    <%= f.text_field :imagem_file_size %>
  </p>
  <p>
    <%= f.label :imagem_updated_at %><br />
    <%= f.datetime_select :imagem_updated_at %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

