<div id="container_adm">
  <p class="title_main">Conjunto de Cidades</p>
  <p><%= link_to 'Novo conjunto de cidade', new_conjunto_cidade_path %></p>
  <table cellpadding="0" cellspacing="0">
  <tr>
    <th>Nome</th>
    <th>Url</th>
    <th></th>
    <th></th>
  </tr>
<% @conjunto_cidades.each do |conjunto_cidade| %>
  <tr>
    <td><%=h conjunto_cidade.nome %></td>
    <td><%=h conjunto_cidade.url %></td>
    <td class="buttom"><%= link_to image_tag('/admin/botao_editar.png', :alt => 'Editar', :title => 'Editar'), edit_conjunto_cidade_path(conjunto_cidade) %></td>
    <td class="buttom"><%= link_to image_tag('/admin/botao_excluir.png', :alt => 'Apagar', :title => 'Apagar'), conjunto_cidade, :confirm => 'Deseja realmente excluir?', :method => :delete %></td>
  </tr>
<% end %>
</table>
  <p><%= link_to 'Novo conjunto de cidade', new_conjunto_cidade_path %></p>
</div>