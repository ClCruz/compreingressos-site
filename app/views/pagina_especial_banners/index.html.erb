<h1>Listing pagina_especial_banners</h1>

<table>
  <tr>
    <th>Link</th>
    <th>Blank</th>
    <th>Ordem</th>
    <th>Imagem file name</th>
    <th>Imagem content type</th>
    <th>Imagem file size</th>
    <th>Imagem updated at</th>
  </tr>

<% @pagina_especial_banners.each do |pagina_especial_banner| %>
  <tr>
    <td><%=h pagina_especial_banner.link %></td>
    <td><%=h pagina_especial_banner.blank %></td>
    <td><%=h pagina_especial_banner.ordem %></td>
    <td><%=h pagina_especial_banner.imagem_file_name %></td>
    <td><%=h pagina_especial_banner.imagem_content_type %></td>
    <td><%=h pagina_especial_banner.imagem_file_size %></td>
    <td><%=h pagina_especial_banner.imagem_updated_at %></td>
    <td><%= link_to 'Show', pagina_especial_banner %></td>
    <td><%= link_to 'Edit', edit_pagina_especial_banner_path(pagina_especial_banner) %></td>
    <td><%= link_to 'Destroy', pagina_especial_banner, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

