<div id="container_adm">
	<p class="title_main">Especiais</p>
	<p><%= link_to 'Novo especial', new_especial_path %></p>
	<table>
	  <tr>
	    <th>Titulo</th>
	    <th>Subtitulo</th>
	    <th>Titulo left</th>
	    <th></th>
	    <th></th>
	  </tr>
	
	<% @especiais.each do |especial| %>
	  <tr>
	    <td><%=h especial.titulo %></td>
	    <td><%=h especial.subtitulo %></td>
	    <td><%=h especial.titulo_left %></td>
	    <td><%= link_to image_tag('/admin/botao_editar.png', :alt => 'Editar', :title => 'Editar'), edit_especial_path(especial) %></td>
	    <td><%= link_to image_tag('/admin/botao_excluir.png', :alt => 'Apagar', :title => 'Apagar'), especial, :confirm => 'Deseja realmente excluir?', :method => :delete %></td>
	  </tr>
	<% end %>
	</table>
	<p><%= link_to 'Novo especial', new_especial_path %></p>
</div>
