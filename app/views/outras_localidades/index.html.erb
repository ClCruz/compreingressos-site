<div id="container_adm">
	<p class="title_main">Outras Localidades</p>
	<p><%= link_to 'Nova outras localidades', new_outras_localidade_path %></p>
	<table>
  		<tr>
  			<th>Cidade</th>
  			<th></th>
  			<th></th>
  		</tr>
		<% @outras_localidades.each do |outras_localidade| %>
  		<tr>
  			<td><%=outras_localidade.home.cidade.nome%></td>
    		<td class="buttom"><%= link_to image_tag('/admin/botao_editar.png', :alt => 'Editar', :title => 'Editar'), edit_outras_localidade_path(outras_localidade) %></td>
    		<td class="buttom"><%= link_to image_tag('/admin/botao_excluir.png', :alt => 'Apagar', :title => 'Apagar'), outras_localidade, :confirm => 'Deseja realmente excluir?', :method => :delete %></td>
  		</tr>
		<% end %>
	</table>
	<p><%= link_to 'Nova outras localidades', new_outras_localidade_path %></p>
</div>
